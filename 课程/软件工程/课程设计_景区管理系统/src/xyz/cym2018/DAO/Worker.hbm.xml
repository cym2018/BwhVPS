<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="xyz.cym2018.DAO.Worker" table="worker">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="assigned"/>
        </id>
        <property name="name"/>
        <property name="salary"/>
        <property name="work"/>
        <property name="state" insert="false"/>
        <property name="starttime" insert="false"/>
        <property name="endtime" insert="false"/>
    </class>
</hibernate-mapping>