<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="xyz.cym2018.DAO.CoutseList" table="courselist">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="native"/>
        </id>
        <property name="coursename" column="coursename"/>
        <property name="semester" column="semester"/>
        <property name="studyhour" column="studyhour"/>
        <property name="credits" column="credits"/>
    </class>
</hibernate-mapping>
