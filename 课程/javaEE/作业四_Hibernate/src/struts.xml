<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="default" namespace="/" extends="struts-default">
        <action name="action1_login" class="xyz.cym2018.struts.Action1_Login">
            <result name="success">index.jsp</result>
            <result name="input">login.jsp</result>
        </action>
        <action name="action2_reg" class="xyz.cym2018.struts.Action2_Reg">
            <result name="success">reg_succ.jsp</result>
            <result name="input">reg.jsp</result>
        </action>
        <action name="action3_logout" class="xyz.cym2018.struts.Action3_Logout">
            <result name="success">index.jsp</result>
        </action>
        <action name="action4_showcourse" class="xyz.cym2018.struts.Action4_ShowSelectCourse">
            <result name="success">show_select_course.jsp</result>
            <result name="login">login.jsp</result>
        </action>
        <action name="action5_selectcourse" class="xyz.cym2018.struts.Action5_SelectCourse">
            <result name="success">select_course.jsp</result>
            <result name="login">login.jsp</result>
        </action>
    </package>
</struts>
